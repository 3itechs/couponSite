RewriteEngine On 
  
RewriteCond %{HTTPS} off
RewriteCond %{REQUEST_URI} !^/[A-F0-9]{32}\.txt(?:\ Comodo\ DCV)?$
RewriteRule (.*) https://%{HTTP_HOST}%{REQUEST_URI} [R=301,L]

# ------- Redirect promostrends to localhost/promostrends ---
Rewritecond %{http_host} ^promostrends  
RewriteCond %{REQUEST_URI} !^/[A-F0-9]{32}\.txt(?:\ Comodo\ DCV)?$
RewriteRule ^(.*) https://www.promosplusdeals.com/$1 [R=301,L] 


RewriteCond %{HTTPS_HOST} ^promosplusdeals.com [NC]
RewriteRule ^(.*)$ https://www.promosplusdeals.com/$1 [L,R=301]

# ------- Redirect every .php to .html  ---
RewriteCond %{THE_REQUEST} (\.php) [NC] 
RewriteCond %{REQUEST_URI} !^/[A-F0-9]{32}\.txt(?:\ Comodo\ DCV)?$
RewriteRule ^(.*)\.php$ https://www.promosplusdeals.com/$1.html [L,R=301]

# ------- Redirect localhost/promostrends/index.html to localhost/promostrends ---
RewriteCond %{THE_REQUEST} (index\.html) [NC] 
RewriteCond %{REQUEST_URI} !^/[A-F0-9]{32}\.txt(?:\ Comodo\ DCV)?$
RewriteRule ^(.*)index\.html$ https://www.promosplusdeals.com/$1 [L,R=301] 

# Url masking for stores.php   as stores/ 
RewriteCond %{REQUEST_URI} !^/[A-F0-9]{32}\.txt(?:\ Comodo\ DCV)?$
RewriteRule ^stores.html$ stores-list.php [NC,L]

# Url masking for categories-list.php   as categories/ 
RewriteCond %{REQUEST_URI} !^/[A-F0-9]{32}\.txt(?:\ Comodo\ DCV)?$
RewriteRule ^categories.html$ categories-list.php [NC,L]

# Url masking for categories.php   as categories/
#RewriteRule ^categories.html/$ categories.html [L,R=301]

# URL Masking for category Detail Page
RewriteCond %{REQUEST_URI} !^/[A-F0-9]{32}\.txt(?:\ Comodo\ DCV)?$
RewriteRule ^categories/(.*)$ category.php?curl=$1 [NC,L]

#RewriteRule ^stores/(.*)-discount-coupon-codes.html$ 
     
RewriteRule ^coupons/(.*)$ store.php?surl=$1 [NC,L]

